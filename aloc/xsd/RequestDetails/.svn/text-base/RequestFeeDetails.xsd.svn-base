<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://treasury.ge.com/schemas/ALOC/APM/RequestFeeDetails.xsd"
	 targetNamespace="http://treasury.ge.com/schemas/ALOC/APM/RequestFeeDetails.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="RequestFeeDetails">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Bank_Name" type="xs:string" minOccurs="0"/>
				<xs:element name="Bank_MDM_Id" type="xs:string" minOccurs="0"/>
				<xs:element name="Request_Id" type="xs:integer" minOccurs="0"/>
				<xs:element name="Site_Prefix" type="xs:string" minOccurs="0"/>
				<xs:element name="Bank_Reference_Number" type="xs:string" minOccurs="0"/>
				<xs:element name="BUC" type="xs:string" minOccurs="0"/>
				<xs:element name="ADN" type="xs:string" minOccurs="0"/>
				<xs:element name="BUC_ADN_Error" type="xs:string" minOccurs="0"/>
				<xs:element name="Fee_Calc_Period_Start_Dt" type="xs:dateTime" minOccurs="0"/>
				<xs:element name="Fee_Calc_Period_End_Dt" type="xs:dateTime" minOccurs="0"/>
				<xs:element name="Payment_Amount" type="xs:decimal" minOccurs="0"/>
				<xs:element name="USD_Equival" type="xs:decimal" minOccurs="0"/>
				<xs:element name="Payment_Amount_String" type="xs:string" minOccurs="0"/>
				<xs:element name="USD_Equival_String" type="xs:string" minOccurs="0"/>
				<xs:element name="Period_Amendments" type="xs:integer" minOccurs="0"/>
				<xs:element name="Total_Amendments" type="xs:integer" minOccurs="0"/>
				<xs:element name="Aloc_Record_Id" type="xs:string" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>