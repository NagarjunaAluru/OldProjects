<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://treasury.ge.com/schemas/ALOC/APM/GetFXRates.xsd"
	 targetNamespace="http://treasury.ge.com/schemas/ALOC/APM/GetFXRates.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="GetFXRates">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Currency_Code" type="xs:string" minOccurs="0"/>
				<xs:element name="FXRate" type="xs:decimal" minOccurs="0"/>
				<xs:element name="FXRate_Source" type="xs:string" minOccurs="0"/>
				<xs:element name="FXRate_Date" type="xs:dateTime" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>