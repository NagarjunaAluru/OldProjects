<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://www.tibco.com/schemas/ALOC/Resources/Schemas/RequestDetails/BidReplyDetails.xsd"
	 targetNamespace="http://www.tibco.com/schemas/ALOC/Resources/Schemas/RequestDetails/BidReplyDetails.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="BidReplyDetails">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Today_Date" type="xs:dateTime" minOccurs="0"/>
				<xs:element name="Pricing_Expiration_Date" type="xs:dateTime" minOccurs="0"/>
				<xs:element name="Bid_Expiration_Date" type="xs:dateTime" minOccurs="0"/>
				<xs:element name="Issuance_Required" type="xs:boolean" minOccurs="0"/>
				<xs:element name="Issuance_Type_flag" type="xs:string" minOccurs="0"/>
				<xs:element name="Notes" type="xs:string" minOccurs="0"/>
				<xs:element name="Bank_Name" type="xs:string" minOccurs="0"/>
				<xs:element name="Bid_Expiration_Time" type="xs:string" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>