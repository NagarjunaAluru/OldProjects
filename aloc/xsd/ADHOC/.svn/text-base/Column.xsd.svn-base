<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://treasury.ge.com/schemas/ALOC/ADHOC/Column.xsd"
	 targetNamespace="http://treasury.ge.com/schemas/ALOC/ADHOC/Column.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="Column">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="UserColumnKey" type="xs:string" minOccurs="0"/>
				<xs:element name="SortOrder" type="xs:string" minOccurs="0"/>
				<xs:element name="SortPriority" type="xs:integer" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>