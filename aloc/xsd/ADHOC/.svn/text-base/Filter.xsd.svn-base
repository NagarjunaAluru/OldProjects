<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://treasury.ge.com/schemas/ALOC/ADHOC/Filter.xsd"
	 targetNamespace="http://treasury.ge.com/schemas/ALOC/ADHOC/Filter.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="Filter">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="UserColumnKey" type="xs:string" minOccurs="0"/>
				<xs:element name="Operator" type="xs:string" minOccurs="0"/>
				<xs:element name="Condition" type="xs:string" minOccurs="0"/>
				<xs:element name="Value1" type="xs:string" minOccurs="0"/>
				<xs:element name="Value2" type="xs:string" minOccurs="0"/>
				<xs:element name="DataType" type="xs:string" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>