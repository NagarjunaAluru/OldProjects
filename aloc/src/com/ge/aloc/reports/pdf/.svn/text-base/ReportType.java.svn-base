package com.ge.aloc.reports.pdf;

/**
 * This is enumeration class to represent Report Type.
 * 
 * @author chaitanya.n
 */
public enum ReportType {
	REQUEST("Request Information");

	private String name;

	/**
	 * Constructor to initialize the name.
	 * 
	 * @param name
	 */
	private ReportType(String name) {
		this.name = name;
	}

	/**
	 * Getter method for name of the report.
	 * 
	 * @return
	 */
	public String getName() {
		return this.name;
	}
}
