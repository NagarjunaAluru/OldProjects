package com.ge.aloc.reports.adhoc;

import com.ge.aloc.OpCode;

/**
 * @author arijit.biswas
 *
 */
public enum AdhocReportTabType {
	MYREPORTS(OpCode.MYREPORTS),
	PUBLISHEDREPORTS(OpCode.PUBLISHEDREPORTS);
	
	private OpCode opCode;

	/**
	 * DashboardView Type constructor.
	 * @param opCode
	 */
	private AdhocReportTabType(OpCode opCode) {
		this.opCode = opCode;
	}

	/**
	 * This is used to get Opcode
	 * getOpcode
	 * @return
	 */
	public OpCode getOpCode() {
		return this.opCode;
	}
}
