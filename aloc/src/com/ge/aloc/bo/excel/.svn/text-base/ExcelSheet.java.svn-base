package com.ge.aloc.bo.excel;

import java.util.ArrayList;
import java.util.List;

/**
 * @author Rajat-Hydus
 *
 */
public class ExcelSheet {

	protected String sheetName;
	protected List<CellDetail> cellDetail;

	/**
	 * Gets the value of the sheetName property.
	 * 
	 * @return
	 *     possible object is
	 *     {@link String }
	 *     
	 */
	public String getSheetName() {
		return sheetName;
	}

	/**
	 * Sets the value of the sheetName property.
	 * 
	 * @param value
	 *     allowed object is
	 *     {@link String }
	 *     
	 */
	public void setSheetName(String value) {
		this.sheetName = value;
	}

	/**
	 * Gets the value of the cellDetail property.
	 * 
	 * <p>
	 * This accessor method returns a reference to the live list,
	 * not a snapshot. 
	 * This is why there is not a <CODE>set</CODE> method for the cellDetail property.
	 * 
	 * <p>
	 * For example, to add a new item, do as follows:
	 * <pre>
	 *    getCellDetail().add(newItem);
	 * </pre>
	 * 
	 * 
	 * <p>
	 * Objects of the following type(s) are allowed in the list
	 * {@link CellDetail }
	 * 
	 * 
	 */
	public List<CellDetail> getCellDetail() {
		if (cellDetail == null) {
			cellDetail = new ArrayList<CellDetail>();
		}
		return this.cellDetail;
	}

}
