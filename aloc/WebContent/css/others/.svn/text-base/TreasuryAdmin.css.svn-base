ul.adminlinks{list-style:none;}
ul.adminlinks li{list-style:none; line-height:30px;}


ul.ddnav,
ul.ddnav li,
ul.ddnav ul {
 list-style: none;
 margin: 0;
 padding: 0;
}

ul.ddnav {
 position: relative;
 z-index: 597;
 float: left;
}

ul.ddnav li {
 float: left;
 min-height: 1px;
 line-height: 1.3em;
 vertical-align: middle;
}

ul.ddnav li.hover,
ul.ddnav li:hover {
 position: relative;
 z-index: 599;
 cursor: default;
}

ul.ddnav ul {
 visibility: hidden;
 position: absolute;
 top: 100%;
 left: 0;
 z-index: 598;
 width: 100%;
 background-color:#eee;
 width:230px;
 border:Solid 1px #ddd;
 padding:10px;
}

ul.ddnav ul li {
 float: none;
 line-height:30px;
 background-color:none;
 border-bottom:Solid 1px #ddd;
}
ul.ddnav ul li a:hover{
  background-color:none!important;
  color:#fff!important;
}
ul.ddnav ul li:hover{
  background-color:#4E7595!important;
  color:#fff!important;
}

ul.ddnav ul ul {
 top: 1px;
 left: 99%;
}

ul.ddnav li:hover > ul {
 visibility: visible;
}

.filterMsg {
	width:auto;
	padding:0px 10px 10px 10px;
	background:#f9f9f9;
	border:1px dashed #ccc;
	margin:10px auto 10px auto;
}

#basicSearchFH div.arrow { background:transparent url(../../img/arrows.png) no-repeat scroll 0px -20px; width:20px; height:20px; display:block; cursor:pointer;}
#basicSearchFH div.up {background:transparent url(../../img/arrows.png); background-position:20px -20px; width:20px; height:20px; display:block; cursor:pointer;}
#basicSearchFH div.down {background:transparent url(../../img/arrows.png); background-position:20px 0px; width:20px; height:20px; display:block; cursor:pointer;}
