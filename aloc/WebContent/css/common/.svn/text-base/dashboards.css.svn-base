/* Search Sort */
#searchSort {
	border:0px solid red;
	width:100%;
	margin-bottom:10px;
}
#searchSort select {
	width:50px!important;
}
#searchSort .leftColSort {
	border:0px solid red;
	width:300px;
	float:left;
}
#searchSort .rightColSort {
	border:0px solid red;
	width:300px;
	float:right;
	text-align:right;
	vertical-align: middle;
}

/* Expand Advance Search */
#basicSearch div.arrow { background:transparent url(../../img/arrows.png) no-repeat scroll 0px -20px; width:20px; height:20px; display:block; cursor:pointer;}

#basicSearch div.up {background:transparent url(../../img/arrows.png); background-position:20px -20px; width:20px; height:20px; display:block; cursor:pointer;}
#basicSearch div.down {background:transparent url(../../img/arrows.png); background-position:20px 0px; width:20px; height:20px; display:block; cursor:pointer;}

/* Expand At a Glance */
#glance div.arrow { background:transparent url(../../img/arrows.png) no-repeat scroll 0px -20px; width:25px; height:20px; display:block; cursor:pointer;}


#glance div.up {background:transparent url(../../img/arrows.png); background-position:20px -20px; width:25px; height:20px; display:block; cursor:pointer;}
#glance div.down {background:transparent url(../../img/arrows.png); background-position:20px 0px; width:25px; height:20px; display:block; cursor:pointer;}

