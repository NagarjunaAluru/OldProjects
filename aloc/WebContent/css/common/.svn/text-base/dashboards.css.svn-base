/*Toggle for Each Row - First TAB*/
       
table tr.srtr1 {background-color:#f4f5f7!important;}
table tr.srtr2 {background-color:#f4f5f7;}
table div.arrow { background:transparent url(../../img/arrows.png) no-repeat scroll 0px -20px; width:20px; height:20px; display:block; cursor:pointer;}

#arrow {
	float:left;
	margin:0px 5px 5px 0px;
}

table div.up {background:transparent url(../../img/arrows.png); background-position:20px -20px; width:20px; height:20px; display:block; cursor:pointer;}
table div.down {background:transparent url(../../img/arrows.png); background-position:20px 0px; width:20px; height:20px; display:block; cursor:pointer;}

table div.pencilNotClicked {background:transparent url(../../img/edit-leg2.gif); no-repeat; width:18px; height:17px; display:block; cursor:pointer;}
table div.pencilClicked {background:transparent url(../../img/edit-selected.gif); width:18px; height:20px; display:block; cursor:pointer;}
table div.exportToExcel {background:transparent url(../../img/excel.gif); no-repeat; width:18px; height:17px; cursor:pointer;}


/*dropdown menu Div hide and show*/
#dropdownBB ul {
    margin: 0px;
    padding: 0;
    list-style: none;
}
#dropdownBB li {
    display: block;
    position: relative;
    float: left;
	list-style:none;
}
#dropdownBB li ul {
    display: none;
	width:600px;
	height:auto;
	padding:10px;
	background:#fff;
	border-radius:5px;
	z-index:9999;
}
#dropdownBB ul li a {
    display: block;
    text-decoration: none;
    color: #333;
    padding: 0px;
	/*padding: 5px 15px 5px 15px;*/
    margin-left: 1px;
    white-space: nowrap;
	text-decoration:underline;
}
#dropdownBB ul li a:hover {
	background: #fff;
}
/*#dropdownBB li:hover ul {
    display: block;
    position: absolute;
}*/
#dropdownBB li:hover li {
    float: none;
    font-size: 11px;
}
#dropdownBB li:hover a { background: #fff; }
#dropdownBB li:hover li a:hover {
    background: #fff;
}

.uppercase { text-transform:uppercase; }



