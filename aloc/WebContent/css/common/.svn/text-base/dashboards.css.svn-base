.popBox {
	width:670px;
	height:auto;
	border:2px solid #000;
	background:#ccc;
	position:absolute;  
	display: none;
	padding: 10px;
	margin-top:20px;
	margin-left:-300px;
	z-index:9999;
}
.popBox:after {
	content:'';
	display:block;
	position:absolute;
	top:-40px;
	left:45%; /*should be set to -border-width x 2 */
	/* width:0;
	height:0; */
	border-color: transparent transparent black transparent; /*border color should be same as div div background color*/
	border-style: solid;
	border-width: 20px;
}

.ltPopBox {
	width:670px;
	height:auto;
	border:2px solid #000;
	background:#f1f9ff;
	position:absolute;  
	display: none;
	padding: 10px;
	margin-top:20px;
	margin-left:-400px;
	z-index:9999;
}
.ltPopBox:after {
	content:'';
	display:block;
	position:absolute;
	top:-40px;
	left:60%; /*should be set to -border-width x 2 */
	/* width:0;
	height:0; */
	border-color: transparent transparent black transparent; /*border color should be same as div div background color*/
	border-style: solid;
	border-width: 20px;
}
