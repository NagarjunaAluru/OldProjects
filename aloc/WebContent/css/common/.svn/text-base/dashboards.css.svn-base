@charset "utf-8";
/* CSS Document */
#nav {
    display:inline;
    text-align:left;
    position:relative;
    list-style-type:none;
	margin:0px!important;
}
#nav ul {
	margin:0px!important;
}
#nav > li {
    float:left;
    padding:0;
	margin:0px!important;
    position:relative;
}
#nav > li > a {
    border:1px solid transparent;
    color:#4F4F4F;
    display:block;
    padding:3px 10px;
    position:relative;
    text-decoration:none;
	width:60%;
}
#nav > li > a:hover {
    background-color:#e4ecf4;
    border-color:#999
}
#nav > li.selected  > a {
    background-color:#FFFFFF;
    border-color:#999999 #999999 #FFFFFF;
    z-index:9999;
}
#nav li div {
    position:relative;
}
#nav li div div {
    background-color:#FFFFFF;
    border:1px solid #999999;
    padding:12px 0;
    display:none;
    font-size:11px;
    margin:0;
    position:absolute;
    top:-1px;
    z-index:1;
    width:240px;
}
#nav li div div.wrp2 {
    width:380px;
}
#nav .sep {
    left:190px;
    border-left:1px solid #E3E3E3;
    bottom:0;
    height:auto;
    margin:15px 0;
    position:absolute;
    top:0;
    width:1px;
}
#nav li div ul {
    padding-left:10px;
    padding-right:10px;
    position:relative;
    width:220px;
    float:left;
    list-style-type:none;
}
#nav li div ul li {
    margin:0;
    padding:0;
}
#nav li ul ul {
    padding:0 0 8px;
}
#nav li ul ul li {
    margin:0;
    padding:0;
}
#nav li ul ul li a {
    color:#0060A6;
    display:block;
    margin-bottom:1px;
    padding:3px 5px;
    text-decoration:none;
    font-size:0.9em;
	border-bottom:1px solid #ccc;
}
#nav li ul ul li a:hover{
    background-color:#276287;
    color:#fff;
}
/* NAV ENDS HERE */
.nav-tabs > li > a{
	background: none repeat scroll 0 0 #A6C2D6;
	color: #FFFFFF;
    position: relative;
    -webkit-border-radius: 4px 4px 0 0;
    -moz-border-radius: 4px 4px 0 0;
    border-radius: 4px 4px 0 0;
    margin-right: 4px;
    padding: 10px 10px;
}
.nav-tabs > li > a:hover {
	background: #376a97;
	border: 1px solid #376a97;
}

