/* ######### PAGE NAVI ############ */
/* ### PAGE NAVI CSS ### */
.pagenavi {	
	font-family:Arial, Helvetica, sans-serif;
	font-size:12px;
	font-weight:normal;
	color:#fff;
	border:0px solid red;
	margin-top:5px;
}
.pagenavi a, .pagenavi span {
	text-decoration: none;
	color:#fff;
	background: #7ba1be;
	padding:5px 10px;
	margin-right:5px;
	border: 1px solid #7ba1be;
}
.pagenavi a:hover, .pagenavi span.current {
	background:#e1e1e1;
	color:#333;
	border-color:#e1e1e1;
}
.pagenavi span.current {
	background:#e1e1e1;
	color:#333;
	border-color:#e1e1e1;
}