/*----- BUTTONS -----*/
.btn{
  /* behavior: url(css/PIE.htc); */position:relative;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  border-radius: 4px;
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
}
.btn-secondary{
  display: inline-block;
  *display: inline;
  padding: 4px 12px;
  margin-bottom: 0;
  margin-left: 5px;
  margin-right: 0px;
  font-size: 13px;
  line-height: 15px;
  color: #666666;
  text-align: center;
  text-shadow: 0 1px 1px rgba(255, 255, 255, 0.75);
  vertical-align: middle;
  cursor: pointer;
  background-color: #d7d7d7;
  *background-color: #e6e6e6;
  gradient: #f2f2f2, #d7d7d7;
  background-image: -moz-linear-gradient(top, #ffffff, #e6e6e6);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#ffffff), to(#e6e6e6));
  background-image: -webkit-linear-gradient(top, #ffffff, #e6e6e6);
  background-image: -o-linear-gradient(top, #ffffff, #e6e6e6);
  background-image: linear-gradient(to bottom, #ffffff, #e6e6e6);
  background-repeat: repeat-x;
  border: 1px solid #cccccc;
  *border: 0;
  border-color: #e6e6e6 #e6e6e6 #bfbfbf;
  border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
  border-bottom-color: #b3b3b3;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
	
}
.btn-secondary:hover{
  color: #5c5f63;
}
.btn-primary {
	color: #ffffff;
	text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
	background-color: #336699;
	gradient: #336699, #1f3f6a;
	background-image:-moz-linear-gradient(top, #276287, #274287);
	background-image:-webkit-gradient(linear, 0 0, 0 100%, from(#276287), to(#274287));
	background-image:-webkit-linear-gradient(top, #276287, #274287);
	background-image: -o-linear-gradient(top, #276287, #274287);
	background-image: linear-gradient(to bottom, #276287, #274287);
	background-repeat: repeat-x;
	border-color:#274287 #274287 #16254c;
	border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
	padding: 4px 12px;
}
.btn-primary:hover {
	background-color: #1F3F6A;
}
.btn-tertiary {
	color: #336699;
	margin-left: 10px !important;
	padding: 5px 12px;
}
