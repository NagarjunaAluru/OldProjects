/*----- TABLES -----*/
table thead {background:#819EB6!important; border-bottom: 1px solid #335B7B;color: #FFFFFF;    line-height: 11px;} 
table .bgtr {background:#819EB6!important; border-radius:0px; color:#fff;} 
table .header {cursor: pointer;}
table .header:after {
	/* content: ""; */
	float: right;
	margin: 10px auto 0;
	border-width: 0 4px 4px;
	border-style: solid;
	border-color: #fff transparent;
	visibility: hidden;
	width:1px;
	display: block;  /* add for bottom indicator */
}

table .header:hover:after {visibility: visible;}
table .headerSortDown:after,
table .headerSortDown:hover:after {
  border-bottom: 4px solid #fff;
  border-left: 4px solid transparent;
  border-right: 4px solid transparent;
  border-top: none;
  visibility: visible;
  filter: alpha(opacity=60);
  -khtml-opacity: 0.6;
  -moz-opacity: 0.6;
  opacity: 0.6;
  width: 1px;
}
table .headerSortUp:after,
table .headerSortUp:hover:after  {
  border-bottom: none;
  border-left: 4px solid transparent;
  border-right: 4px solid transparent;
  border-top: 4px solid #fff;
  visibility: visible;
  filter: alpha(opacity=60);
  -khtml-opacity: 0.6;
  -moz-opacity: 0.6;
  opacity: 0.6;
  width: 1px;
}
table thead .header.nosort:hover:after {visibility: hidden;}
table thead .header.nosort:after {margin:0px auto 0 !important;border-width:0;}
table thead .header.nosort {text-align: center;padding: 3px 0;}
table .headerSortUp {background: url(../../img/arrow-up.png) 95% 50% no-repeat #4E7595; text-shadow: none;}
table .headerSortDown {background: url(../../img/arrow-down.png) 95% 50% no-repeat #4E7595; text-shadow: none;}
.table th, .table td {border-top: 1px solid #A6C2D6;line-height: 18px;padding: 8px;text-align: left;vertical-align: top;}
.table-bordered {border-left:0;border-right:0;border-bottom:1px solid #819EB6 ;font-size: 13px;}
.table-striped tbody tr:nth-child(2n+1) td {background-color: #F1F9FF;}
.table-striped tbody tr:hover td {background-color: #E6F4FF;}
.table-striped tbody tr:hover td#bidsRow {background: #E6F4FF url(../../img/bids-bkg.png) top left repeat-y!important;}
.table-striped tbody tr:hover td#perforRow {background: #E6F4FF url(../../img/perfor-bkg.png) top left repeat-y!important;} 
.table-striped tbody tr:hover td#advanceRow {background: #E6F4FF url(../../img/advance-bkg.png) top left repeat-y!important;}
.table-striped tbody tr:hover td#financialRow {background: #E6F4FF url(../../img/financial-bkg.png) top left repeat-y!important;}
.table-striped tbody tr:hover td#docRow {background: #E6F4FF url(../../img/doc-bkg.png) top left repeat-y!important;}
.table-striped tbody tr:hover td#noPurposeRow {background: #E6F4FF top left repeat-y!important;}
.table-bordered th + th, 
.table-bordered td + td, 
.table-bordered th + td, 
.table-bordered td + th {border-left: 1px solid #A6C2D6;}
.table-bordered thead th + th, 
.table-bordered thead td + td, 
.table-bordered thead th + td, 
.table-bordered thead td + th {border-left: 0;}

.table thead tr th{border: 1px solid #A6C2D6;line-height: 18px;padding: 8px;text-align: left;vertical-align: top; font-weight: normal;}
