#logindiv{padding:10px; height:350px; font-family:Arial; font-size:12px;
background: #f2f2f2;
	/* chrome 2+, safari 4+; multiple color stops */
	background-image:-webkit-gradient(linear, left bottom, left top, color-stop(0.32, #e2eaec), color-stop(0.66, #f4f4f4), color-stop(0.83, #f4f4f4));
	/* chrome 10+, safari 5.1+ */
	background-image: -webkit-linear-gradient(#e2eaec, #f4f4f4, #f4f4f4);
	/* firefox; multiple color stops */
	background-image: -moz-linear-gradient(top,#e2eaec, #f4f4f4, #f4f4f4);
	/* ie 6+ */
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#e2eaec', endColorstr='#f4f4f4');
	/* ie8 + */
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr='#e2eaec', endColorstr='#f4f4f4')";
	/* ie10 */
	background-image: -ms-linear-gradient(#e2eaec, #f4f4f4, #f4f4f4);
	/* opera 11.1 */
	background-image: -o-linear-gradient(#e2eaec, #f4f4f4, #f4f4f4);
	/* The "standard" */
	background-image: linear-gradient(#e2eaec, #f4f4f4, #f4f4f4);
}
