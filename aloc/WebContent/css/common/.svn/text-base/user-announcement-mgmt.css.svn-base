@charset "utf-8";
/* CSS Document */

#userMgmt {
	width:99%;
	padding:0px;
	border:0px solid red;
	margin-bottom:20px;
}
#userMgmt .row {margin-bottom: 8px;}
#userMgmt .leftBox {
	width:380px;
	float:left;
	border:0px solid red;
}
#userMgmt .midBox {
	width:280px;
	float:left;
	background:#f2f2f2;	
	margin-left:13px;
	border:0px solid red;
	padding:5px;
}
#userMgmt .rightBox {
	width:220px;
	float:right;
	padding:5px;
	padding:5px;
	background:#f2f2f2;	
	border:0px solid red;
}
.required-fields-para{font-size: 11px;font-weight:normal;color:#AE2C2C; width:200px; z-index:9999; margin-top:0px; margin-left:455px; position:absolute;}

#userMgmt li {
	line-height:50px;
}

form div.wordcount {
    float: left;
    clear: both;
    padding: 2px;
    border-top: 0px none;
    font-family : Helvetica, Arial, sans-serif;
	color: #777777;
}
.deleteIcon {background: url("../../img/delete.gif") no-repeat scroll left top;color: #AE2C2C;display: block;font-weight: bold;height: 18px;margin-left:-5px;auto;text-indent: -9999px;width: 17px;}

.userDownload{
	margin-top:-15px; margin-left:25px; position:absolute;
}