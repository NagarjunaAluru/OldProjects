<%@ page contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ page errorPage="/jsp/common/error.jsp"%>
<%@ taglib prefix="s" uri="/struts-tags"%>
<%@include file="../common/includeCommonScripts.jsp" %> 
<div class="container main">
	<s:form action="" id="formId">
	<div class="row">
			<div class="span12">
			<div class="modal-header">
				<h3><s:text name="label.userannouncementmgmt.hyperlink"/></h3>
			</div>
			<div class="modal-body">
			<form>
			<p><s:text name="label.userannouncementmgmt.hyperlinkText"/></p>
			<div class="form-row">
			<label><s:text name="label.userannouncementmgmt.hyperlinkURL"/></label>
				<s:textfield name="userAnnouncement.hyperLink" cssClass="span5" id="hyperLink" />
			</div>
			</form>
			</div>
			<div class="modal-footer">
				<a href="#derivatives-table" class="btn left" data-dismiss="modal"><s:text name="label.request.Submit"/></a>
				<a href="#" class="btn-tertiary cancel" data-dismiss="modal" onclick="javaScript:window.close();"><s:text name="label.request.common.cancel" /></a>
			</div>
			</div>
		</div> 
	</s:form>
</div>
