<%@ taglib prefix="s" uri="/struts-tags"%>
<div id="footer">
	<div id="footer-menu">
		<div class="left-col-fm">
			<s:text name="label.header.geConfidential" />
		</div>
		<!-- left col logo ends here -->
		<s:url action="contactUs.action" namespace="/int" encode="true" var="contactUsURL"/>
		<s:url action="help.action" namespace="/int" encode="true" var="helpURL"/>
		<div class="right-col-fm">
			<a href="<s:property value="#helpURL"/>"><s:text name="label.header.help" /></a> <b>|</b> <a
				href='<s:property value="#contactUsURL"/>'><s:text name="label.header.contactUs" /></a>
		</div>
		<!-- right col logo ends here -->

		<div class="clear"></div>
	</div>
	<div id="footer-credits">
		<div class="left-col-fc"></div>
		<div class="clear"></div>
	</div>
	<!-- footer credits ends here -->
</div>