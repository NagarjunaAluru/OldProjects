<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://treasury.ge.com/schemas/ICFP/UserInfo.xsd"
	 targetNamespace="http://treasury.ge.com/schemas/ICFP/UserInfo.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="User_Info">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Sso_Id" type="xs:string" minOccurs="0"/>
				<xs:element name="First_Name" type="xs:string" minOccurs="0"/>
				<xs:element name="Last_Name" type="xs:string" minOccurs="0"/>
				<xs:element name="Email_Address" type="xs:string" minOccurs="0"/>
				<xs:element name="Phone" type="xs:string" minOccurs="0"/>
				<xs:element name="User_Name" type="xs:string" minOccurs="0"/>
				<xs:element name="Title" type="xs:string" minOccurs="0"/>
				<xs:element name="Assigner_Sso_Id" type="xs:string" minOccurs="0"/>
				<xs:element name="Assigner_First_Name" type="xs:string" minOccurs="0"/>
				<xs:element name="Assigner_Last_name" type="xs:string" minOccurs="0"/>
				<xs:element name="Assigner_Group" type="xs:string" minOccurs="0"/>
				<xs:element name="Business_Unit" type="xs:string" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="User_Information">
		<xs:sequence>
			<xs:element ref="User_Info" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>