.zdate td,
.zdate th{ text-align: center; padding: 5px 0 }
.zdate td{ cursor: pointer }
.zdate .dp_daypicker,
.zdate .dp_monthpicker,
.zdate .dp_yearpicker{ margin-top: 3px }
.zdate .dp_daypicker td,
.zdate .dp_daypicker th,
.zdate .dp_monthpicker td,
.zdate .dp_yearpicker td{ background: #fff; width: 35px; }
.zdate .dp_header td{ color: #FFF;border:0;}
.zdate .dp_header .dp_previous,
.zdate .dp_header .dp_next{ width: 30px }
.zdate .dp_header .dp_caption{ font-weight: bold; }
.zdate .dp_header .dp_hover{ background: #63839D; color: #FFF; -webkit-border-radius: 3px; -moz-border-radius: 3px; border-radius: 3px }
.zdate .dp_header .dp_blocked{ color: #888; cursor: default }
.zdate .dp_daypicker th{ background:#4E7595;color: #FFFFFF;}
.zdate td.dp_not_in_month{ background: #F3F3F3; color: #CDCDCD; cursor: default }
.zdate td.dp_weekend{ }
.zdate td.dp_weekend_disabled {color: #CCC; cursor: default }
.zdate td.dp_selected{background: #A6C2D6; color: #FFF }
.zdate td.dp_selected.dp_hover{background: #A6C2D6; color: #000 }
.zdate td.dp_week_number{background: #4E7595; color: #555; cursor: text; font-style: italic }
.zdate .dp_monthpicker td{width: 33%;font-size: 13px; vertical-align: middle; }
.zdate .dp_yearpicker td{width: 33%;font-size: 13px; vertical-align: middle; }
.zdate td.dp_current{color: #BB4F00 }
.zdate td.dp_disabled{background: #F3F3F3; color: #CDCDCD; cursor: default }
.zdate td.dp_hover{background: #F1F9FF; }
button.zdate_Icon{ width: 25px; height: 25px; background: url('../img/calendar.gif') no-repeat left top; text-indent: -9000px; border: none; margin: 0 0 0 5px; cursor: pointer }
button.zdate_Icon_Inside{ margin: 0 3px 0 10px; position: relative }
