/*Silo div show*/


 html>body div.scrollable tbody {
            overflow: auto;
         }
 
         table.resizable th{
            text-align:center;
            padding: 0px 5px 0px 5px;
         }
 
         /* if mozilla, add 10 for the scrollbar */
         html>body th.scrollbarCol {
            width:10px;
         }
 
         table.resizable td{
            padding: 5px;
         }
 
         table.resizable{
            table-layout:fixed;
         }
 
         table.resizable input{
            width:100%;
         }
 
         table.resizable textarea{
            width:100%;
         }
 
         .nowrap {
             white-space:nowrap;
         }
.tr1 {
	background:#fff;
	border:1px solid #f2f2f2;
}
.tr2 {
	background:#f4f5f7;
	border:1px solid #f2f2f2;
}
         
         
 
         /* needed for IE */
         table.tabular th.scrollbarCol {
            background-color:transparent; 
         }
 
         table.tabular {
	font-family:GE Inspira Regular;
	font-weight:normal;
	font-size:12px;
	color:#666666;
         }
 
         table.tabular thead {
	font-family:GE Inspira Regular;
