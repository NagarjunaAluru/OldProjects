/* ### MAIN NAVIGATION ### */
#main-menu { width:100%; border:none; margin:0px;}

#main-menu ul#nav {
	list-style: none;
	padding: 0;
	margin: 0;
}
#main-menu ul#nav li a {
	display: block;
	font-weight:bold;
	padding: 6px 8px;
	_padding: 6px 6px;
	color: #fff;
	text-decoration: none;
	font-family:GE Inspira Regular;
	font-size:14px;
	text-transform:capitalize;
}
#main-menu ul#nav li a:hover{
	color: #fff;
	background: #69839b;
	}
#main-menu ul#nav li {
	float: left;
	position: relative;
	list-style:none;
	display: inline; 
}
#main-menu li ul {
	display: none;
	position: absolute;
	width:100px;
	top: 0;
	left: 0;
	font-weight: normal;
	padding: 1px 0 10px 0;
	margin-left:-1px;
}
#main-menu ul#nav li ul.sub li{
	border-width:0 1px 1px 1px!important;
	width:150px;
	border-bottom:1px solid #666;
}
#main-menu ul#nav li ul.sub li a{
	font-weight: normal!important;	
}
#main-menu li>ul {
	top: auto;
	left: auto;
}
#main-menu li:hover ul, li.over ul {
	display: block;
}


