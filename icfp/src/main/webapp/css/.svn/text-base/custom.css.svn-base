/*----- GENERAL -----*/

body,html{background:url(../img/bg-pat.png) repeat #E6E6E6;}
@font-face {
        font-family: "GE Inspira Regular";
        font-style: normal;
       src: url('../font/GEInspRg.eot?#iefix') format('embedded-opentype'); /* for IE 6 */
        src: local("GE Inspira Regular"), url("../font/GEInspRg.ttf") format("truetype"); /* non-IE */
}
@font-face {
        font-family: "GE Inspira Medium";
        font-style: normal;
        src: url('../font/GEInspMd.eot?#iefix') format('embedded-opentype'); /* for IE 6 */ 
        src: local("GE Inspira Medium"), url("../font/GEInspMd.ttf") format("truetype"); /* non-IE */
} 
h1, h2, h3, h4, h5{font-family:"GE Inspira",Arial;}
h1{font-size:18px;color:#376A97;line-height:26px;margin-bottom:10px;}
h1 span{color:#666;}
h2{font-size:14px;}
a{color:#BB4F00;}
a, a:hover, a:active, a:focus, button{outline:0 none !important;}
.red{color:red;}
.bold{font-weight:bold;}
.left{float:left;}
.right{float:right;}
.clear{clear:both;}
.container{background:#fff;padding:0 10px;/* height:auto !important;height:100%;min-height:100%; margin-bottom:-64px; */ box-shadow:0 2px 3px #787878; behavior: url(PIE.htc);position:relative;border-radius: 0 0 20px 20px;} 
.page-title{color:#000;margin-left:4px;}
.btn{position:relative;overflow:hidden;
-moz-border-radius:6px;-webkit-border-radius:6px;border-radius:6px;behavior: url(PIE.htc);}
.btn.disabled {color: #AAAAAA;}
#top-container{width:960px;margin:0 auto;background:#fff;padding-bottom: 10px;margin-left:-10px;}
section {padding-bottom: 60px;}
table th{vertical-align: bottom;}
table thead{background:#E6F4FC !important; border: 1px solid #A6C8DA;}
table thead tr{background:#E6F4FC !important; border: 1px solid #A6C8DA;}

/*----- WELCOME BAR -----*/
#welcomebar{height:32px;padding-top:5px;}
#welcomebar a{display:block;height:32px;width:92px;margin-right:20px;}
#welcomebar p{padding:7px 0 0 0;font-weight:bold; font-size:12px;margin:0;}
#welcomebar p span{color:#376A97;}
.searchbar{margin-right:10px;margin-top:3px;}
.searchbar .add-on{color:#376A97;font:bold 12px "ge inspira", arial;padding-top:2px;   height: 14px;}
.searchbar input{height:12px;}

/*----- HEADER -----*/
#header {background: url(../img/header_bg.jpg);border-bottom:4px solid #699AB3;height: 67px;position:relative;}
.topbar-wrapper{width:510px;float:right;margin-top:-13px;top:50%;}
.topbar-wrapper .topbar {margin:0; height:29px;}
.topbar-wrapper .topbar .topbar-inner {padding:0;height:40px;behavior: url(PIE.htc);position:relative;}
.topbar-inner {background: url("../img/navbg.png") repeat scroll 0 0 transparent;
-pie-background: linear-gradient(#5C8DB6, #3b709c);
filter: "progid:DXImageTransform.Microsoft.gradient(enabled=false)" !important;}
.right-topbar{float:right;position:relative;top:50%;margin:-8px 10px 0 20px;color:#fff;}
.right-topbar a{font:bold 12px "ge inspira";color:#fff; }
.topbar h3 a{margin:0;}
.topbar li a{font-size:16px;font-family:"GE Inspira",Arial;color:#fff;}
.topbar .first{-webkit-border-radius: 3px 0 0px 3px;-moz-border-radius: 3px 0 0px 3px;border-radius: 3px 0 0px 3px;}
.logo{color: #fff;float: left;font-family:ge inspira;font-size: 85px;font-weight: bold;letter-spacing: 5px;line-height: 65px;opacity: 0.1;filter: alpha(opacity=10);position:absolute;}
.logo2{color:#fff;font-size:16px;font-family:ge inspira;position:absolute;top:50%;margin:-12px 0 0 13px;}
.logo2 span{font-weight:bold;font-size:28px;}
.dashdesc{margin-left:4px!important;margin-bottom:18px;}

/*----- NAV -----*/
.topbar div > ul a:hover, .nav a:hover {background: url("../img/navbg-hover.png") repeat scroll 0 0 transparent ;}
.dropdown-menu a:hover{background:none !important;}
.topbar div > ul .active > a, .nav .active > a{background: url("../img/navbg-hover.png") repeat scroll 0 0 transparent !important;} 
.iefix{-pie-background: linear-gradient(center top ,rgba(49, 111, 154, 0.8)  0%, rgba(97, 146, 173, 0.8)100%);
behavior: url(PIE.htc);}

.topbar div > ul a, .nav a {line-height: 8px;}
.topbar-wrapper .topbar .topbar-inner {height: 29px;}
a.menu:after, .dropdown-toggle:after { margin-top: 3px;}


/*----- DROPDOWN -----*/
.menu-dropdown, .dropdown-menu{}
.topbar div > ul .menu-dropdown, 
.nav .menu-dropdown, 
.topbar div > ul .dropdown-menu, 
.nav .dropdown-menu { top: 40px;background:#6395bc;}
.menu-dropdown, .dropdown-menu {overflow:visible;
  -webkit-border-radius:  6px;-moz-border-radius:  6px;border-radius: 6px;}
.topbar div > ul .menu-dropdown li a, 
.nav .menu-dropdown li a, 
.topbar div > ul .dropdown-menu li a, 
.nav .dropdown-menu li a {color: #fff;text-shadow: none;}
.topbar div > ul a.menu:hover,
.nav a.menu:hover,
.topbar div > ul li.open .menu,
.nav li.open .menu,
.topbar div > ul .dropdown-toggle:hover,
.nav .dropdown-toggle:hover,
.topbar div > ul .dropdown.open .dropdown-toggle,
.nav .dropdown.open .dropdown-toggle {background: url("../img/navbg-hover.png") repeat scroll 0 0 transparent;
filter: "progid:DXImageTransform.Microsoft.gradient(enabled=false)";}
.topbar div > ul .menu-dropdown .divider, .nav .menu-dropdown .divider, .topbar div > ul .dropdown-menu .divider, .nav .dropdown-menu .divider {background-color: #234a70;border-color: #9bbbd0;}
.arrow-up {background: url("../img/nav-arrow.png") no-repeat;height: 11px;left: 15px;position: relative;top:-15px;width: 23px;}
.topbar div > ul, .nav {margin: 0;}

/*----- BREADCRUMB -----*/
.breadcrumb {background:none;border:0; padding: 7px 5px;filter: "progid:DXImageTransform.Microsoft.gradient(enabled=false)";}


/*----- TABS -----*/
.tabs {border-bottom-width: 0;margin-bottom: 0;}
.tabs li a{border-bottom-width: 0 !important;}
.tabs > li {position: relative;top: 0px;}
.pill-content table{float:left;
-moz-border-radius:0px 4px 4px 4px;-webkit-border-radius:0px 4px 4px 4px;border-radius:0px 4px 4px 4px;}
.odd{background:#F9F9F9;} /* dumb ie */

/*----- TOOLTIPS -----*/
.twipsy-inner {background: url("../img/tooltip-bg.gif") repeat-x scroll left bottom #6e87a0;border: 1px solid #C1CDD7;border-radius: 8px 8px 8px 8px;color: #FFFFFF;max-width: 200px;padding: 3px 8px;text-align: left;text-decoration: none;behavior: url(PIE.htc);}
.twipsy-inner span{ border-bottom: 1px solid #9cb0c3;display: block;font: bold 11px arial;margin-bottom: 5px;font:bold 13px/24px arial;color:#fff;}
.twipsy.above .twipsy-arrow {bottom: 0;left: 50%;margin-left: -5px;border-left: 5px solid transparent;border-right: 5px solid transparent;border-top: 5px solid #3B5976;}
.twipsy.fade.in {filter: alpha(opacity=100);-khtml-opacity: 1;-moz-opacity:1;opacity: 1;z-index:99999;}


.reviewers{ list-style: none outside none; margin: 10px 0 0 0; display:none;}
.reviewers li{border-bottom:1px solid #DADADA;line-height: 17px;list-style: none;margin-bottom: 3px;padding-bottom: 3px;}
td .view-reviewers:hover{text-decoration:none;}
td .view-reviewers:after {
  content: "";
  float: right;
  margin-top: 7px;
  border-width: 4px 4px 0px;
  border-style: solid;
  border-color: #808080 transparent;
}
td .active{color:#00438a;}
td .active:after{
  content: "";
  float: right;
  margin-top: 7px;
  border-width: 0px 4px 4px;
  border-style: solid;
  border-color: #808080 transparent;
}


/*----- PROGRESS METER -----*/
.preaudit{margin-top:10px;}
.preaudit p{font-family:GE inspira, Helvetica, Arial, sans-serif;color: #6A6969;}
.preaudit p span{width:190px;;display:inline-block;font-weight:bold;}
.bigfont{color: #6A6969;font-size: 40px;padding-top: 9px; padding-left:3px;width: 185px!important;}
.ui-widget-header {background: url("../img/wave.png") repeat-x scroll 50% 50% #5C9CCC;border: 1px solid #4297D7;color: #FFFFFF;font-weight: bold;}
/* .audit{width:325px;} */
.pbar{ margin-left: 45px;width: 380px;}
.subcan{text-align:right;width: 125px;}
.reminder{padding-left:0;}
.alert-message{display:none;}
.alert-message p a{font-weight:bold;color: #fff;}
.pending{display:inline-block;width:100%; margin-top: 6px;}
.meter .nopending{color: #989797;margin: 0;padding: 5px;text-align: center;}
.pending .left{margin-left:8px;display:block;}
.pending .right{margin-right:8px;}
.meter {
        height: 25px;  /* Can be anything */
        position: relative;
        background: #ddd;
        border-radius: 15px;
        padding: 3px;
		border:1px solid #ccc;
        -webkit-box-shadow: inset 0 3px 4px rgba(9, 9, 9, 0.3);
        -moz-box-shadow   : inset 0 3px 4px rgba(9, 9, 9, 0.3);
        box-shadow        : inset 0 3px 4px rgba(9, 9, 9, 0.3);
		behavior: url(PIE.htc);
}
.meter > span {
        display: block;
        height: 100%;
		border-radius: 35px;
		background: url("../img/meter-bg.jpg") repeat-x scroll 0 0 transparent;		 
		position: relative;
        overflow: hidden;
		color:#fff;
		text-align:right;
		behavior: url(PIE.htc);
}
.meter > span a{text-decoration:none;color:#fff;padding-right:10px;font-size:11px;padding-top:5px;display:block;}

