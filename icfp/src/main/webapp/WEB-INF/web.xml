	<!-- Uncomment follwing filter to use GE LDAP Security framework -->
	<filter>
		<filter-name>AccessFilter</filter-name>
		<filter-class>com.hydus.wff.ge.auth.GELDAPAndUserInfoAccessFilter</filter-class>
		<init-param>
			<param-name>loginPage</param-name>
			<param-value>login.do?login</param-value>
		</init-param>
		<init-param>
			<param-name>com.ge.applicationId</param-name>
			<param-value>ICFFO</param-value>
		</init-param>
		<init-param>
			<param-name>com.ge.webserviceServerAddress</param-name>
			<param-value>trstsqasfwapv01.treasury.corp.ge.com:33217</param-value>
		</init-param>
	</filter> 
	<filter> 
		<filter-name>ResponseOverrideFilter</filter-name> 
		<filter-class>org.displaytag.filter.ResponseOverrideFilter</filter-class> 
	</filter>
	<filter>
	    <display-name>NoCacheHeaderFilter</display-name>
	    <filter-name>NoCacheHeaderFilter</filter-name>
	    <filter-class>com.ge.icfp.util.NoCacheResponseFilter</filter-class>
    </filter>
    <filter-mapping>
	    <filter-name>NoCacheHeaderFilter</filter-name>
	    <url-pattern>*.jsp</url-pattern>
    </filter-mapping>
	<filter-mapping>
		<filter-name>AccessFilter</filter-name>
		<url-pattern>*.do</url-pattern>
	</filter-mapping>
	
	<filter-mapping>
		<filter-name>ResponseOverrideFilter</filter-name> 
		<url-pattern>*.do</url-pattern> 
	</filter-mapping>
