<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>

<tlibversion>1.0</tlibversion>
	<jspversion>1.2</jspversion>
	<shortname>hwf-rules</shortname>
	<uri>hwf-tmirules</uri>
	<info>Use Common Rules To Determine Conditional Actions</info>
	<tag>
	    <name>isTrue</name>
		<tagclass>com.hydus.wff.core.tags.HWFRulesTagIsTrue</tagclass>
		<bodycontent>JSP</bodycontent>
		<info></info>
		<attribute>
			<name>ruleSchema</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>ruleName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>parameters</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
	    <name>isFalse</name>
		<tagclass>com.hydus.wff.core.tags.HWFRulesTagIsFalse</tagclass>
		<bodycontent>JSP</bodycontent>
		<info></info>
		<attribute>
			<name>ruleSchema</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>ruleName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>parameters</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>