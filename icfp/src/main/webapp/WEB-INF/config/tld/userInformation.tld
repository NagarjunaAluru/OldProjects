<?xml version="1.0" encoding="ISO-8859-1" ?>

<taglib version="2.0" xmlns="http://java.sun.com/xml/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd">
	<tlib-version>1.0</tlib-version>
	<short-name>ICFP User Details</short-name>
	<uri>http://ge.com/icfp/taglibs/userInformation</uri>
	<display-name>ICFP User Data Tags and Functions</display-name>
	<description>ICFP User Data Tags and Functions</description>

	<function>
		<name>title</name>
		<function-class>com.ge.icfp.tag.UserInformationManager</function-class>
		<function-signature>java.lang.String title(java.lang.String)</function-signature>
	</function>
	
	<function>
		<name>fullName</name>
		<function-class>com.ge.icfp.tag.UserInformationManager</function-class>
		<function-signature>java.lang.String fullName(java.lang.String, java.lang.String)</function-signature>
