<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>

<tlibversion>1.0</tlibversion>
	<jspversion>1.2</jspversion>
	<shortname>hwf-security</shortname>
	<uri>hwf-securitytag</uri>
	<info>Security custom tags</info>
	<tag>
	    <name>hasRoles</name>
		<tagclass>com.hydus.wff.core.tags.HasRoles</tagclass>
		<bodycontent>JSP</bodycontent>
		<info>checks the authorization</info>
		 <attribute>
			<name>appName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		<attribute>
			<name>domainName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>domainType</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>compName</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>compType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
	</tag>
</taglib>