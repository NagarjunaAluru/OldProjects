<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>

<tlibversion>1.0</tlibversion>
	<jspversion>1.2</jspversion>
	<shortname>tmiwf-desktop</shortname>
	<uri>tmiwf-desktoptag</uri>
	<info>desktop custom tags</info>
	<tag>
	    <name>display</name>
		<tagclass>com.hydus.wff.displaytag.HWFDisplayTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<info></info>
		<attribute>
			<name>dataProvider</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>uri</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>pageSize</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		  <attribute>
			<name>tableCSS</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>colBodyCSS</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>colHeaderCSS</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>refreshUrl</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>checkbox</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>pageName</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>filter</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
	</tag>
	<tag>
	    <name>PropertyManager</name>
		<tagclass>com.hydus.wff.core.tags.PropertyManagerTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<info>formats the date in a specified format</info>
		<attribute>
			<name>filename</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>propName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>propValue</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
	</tag>
	<tag>
	    <name>ProcessTabs</name>
		<tagclass>com.hydus.wff.core.tags.ProcessTabs</tagclass>
		<bodycontent>JSP</bodycontent>
		<info>Displays tabs based on process type reading process config files</info>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>pageContentId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>contextURL</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>processType</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>taskName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>wfTemplate</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>appendToURL</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>showNextActivity</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>ruleParams</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
	</tag>
	<tag>
	    <name>ProcessComponents</name>
		<tagclass>com.hydus.wff.core.tags.ProcessComponents</tagclass>
		<bodycontent>JSP</bodycontent>
		<info>Displays components based on process type and taskname reading process config files</info>
		 <attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>processType</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>taskName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
		 <attribute>
			<name>ruleParams</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		 </attribute>
	</tag>
	<tag>
       		<name>legSummary</name>
       		<tagclass>com.ge.icfp.tag.LegSummaryTag</tagclass>	
       		<bodycontent>empty</bodycontent>
       		<info>Display Leg Summary</info>
       		<attribute>
       			<name>name</name>
       			<required>true</required>
       			<rtexprvalue>true</rtexprvalue>
       		</attribute>
       		<attribute>
       			<name>property</name>
       			<required>true</required>
       			<rtexprvalue>true</rtexprvalue>
       		</attribute>
       		
       		<attribute>
       			<name>viewType</name>
       			<required>true</required>
       			<rtexprvalue>true</rtexprvalue>
       		</attribute>
       		
       		<attribute>
       			<name>addLegButton</name>
       			<required>false</required>
       			<rtexprvalue>true</rtexprvalue>
       		</attribute>
       		<attribute>
       			<name>tableId</name>
       			<required>false</required>
       			<rtexprvalue>true</rtexprvalue>
       		</attribute>
       		<attribute>
       			<name>styleClass</name>
       			<required>false</required>
       			<rtexprvalue>true</rtexprvalue>
       		</attribute>
       	</tag>
</taglib>