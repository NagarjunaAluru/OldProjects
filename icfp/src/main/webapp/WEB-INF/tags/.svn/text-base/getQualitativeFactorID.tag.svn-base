<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@attribute name="name" required="true" rtexprvalue="true" %>
<c:choose>
<c:when test="${name eq 'Regulatory Risk'}">1</c:when>
<c:when test="${name eq 'Legal Governance Risk'}">2</c:when>
<c:when test="${name eq 'Sovereign Risk'}">3</c:when>
<c:when test="${name eq 'Reputational Risk'}">4</c:when>
<c:when test="${name eq 'Finance Risk'}">5</c:when>
<c:when test="${name eq 'Operational Risk - Initial'}">6</c:when>
<c:when test="${name eq 'Operational Risk - Ongoing'}">7</c:when>
<c:when test="${name eq 'Tax Risk'}">8</c:when>
<c:when test="${name eq 'Legal Risk'}">9</c:when>
<c:when test="${name eq 'Transfer Pricing Risk'}">10</c:when>
<c:when test="${name eq 'Regulatory/Jurisdictional Reviews Risk'}">11</c:when>
</c:choose>