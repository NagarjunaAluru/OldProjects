

#Default log level to DEBUG. Other levels are INFO and ERROR.
log4j.rootLogger=WARN,ROOT
log4j.appender.ROOT=org.apache.log4j.RollingFileAppender
log4j.appender.ROOT.File=/web/SUNWappserver_9.1/domains/icffo/ICFP_GELibAPI.log
log4j.appender.ROOT.MaxFileSize=1000KB
#Keep 5 old files around.
log4j.appender.ROOT.MaxBackupIndex=5
log4j.appender.ROOT.layout=org.apache.log4j.PatternLayout
#Most useful format
log4j.appender.ROOT.layout.ConversionPattern=%d %-5p %c - %m%n
log4j.additivity.com.ge.corporate.sherlock=false
log4j.category.org.apache.commons= ERROR, ROOT
log4j.category.org.apache.struts.util= ERROR, ROOT




