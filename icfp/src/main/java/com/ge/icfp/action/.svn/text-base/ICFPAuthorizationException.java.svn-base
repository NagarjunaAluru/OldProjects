package com.ge.icfp.action;

/**
 * @author prithivi.dhamotharan
 *
 */
public class ICFPAuthorizationException extends ICFPException {

	private static final long serialVersionUID = 1008342257896694836L;

	/**
	 * 
	 * @param code
	 * @param message
	 * @param cause
	 */
	public ICFPAuthorizationException(String code, String message,
			Throwable cause) {
		super(code, message, cause);
	}

	/**
	 * 
	 * @param code
	 * @param message
	 */
	public ICFPAuthorizationException(String code, String message) {
		super(code, message);
	}

	/**
	 * 
	 * @param code
	 * @param cause
	 */
	public ICFPAuthorizationException(String code, Throwable cause) {
		super(code, cause);
	}

	/**
	 * 
	 * @param code
	 */
	public ICFPAuthorizationException(String code) {
		super(code);
	}
}
